  "colorizers": [
    {
      "name": "Severity (for dark theme)",
      "rules": [
        {
          "name": "Highlight",
          "matcher": {
            "columnName": "msg",
            "pattern": "(Successfully completed deployment.*)|(EJB Deployed EJB with JNDI name.*)"
          },
          "result": {
            "name": "Highlight",
            "foregroundColor": "#50e6ffff",
            "backgroundColor": "#002042ff"
          }
        },
        {
          "name": "Error",
          "matcher": {
            "columnName": "severity",
            "pattern": "[Ee]rror"
          },
          "result": {
            "name": "Red",
            "foregroundColor": "#ff2020ff",
            "backgroundColor": null
          }
        },
        {
          "name": "Warn",
          "matcher": {
            "columnName": "severity",
            "pattern": "[Ww]arn(ing)?"
